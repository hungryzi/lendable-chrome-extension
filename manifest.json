{
  "name": "Lendable",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Detect lendable e-books",
  "homepage_url": "http://extensionizr.com",
  "icons": {
    "16": "icons/lendable16.png",
    "48": "icons/lendable48.png",
    "128": "icons/lendable128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["https://www.amazon.com/*"],
      "js": ["js/content.js"]
    }
  ]
}